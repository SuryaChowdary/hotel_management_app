<% if @client.errors.any? %>
  <% if params[:action] == "create" %>
    $("#newerrors").html("<%= j(render 'clients/errors', client: @client) %>");
  <% elsif params[:action] == "update" %>
    $("#editerrors-<%= @client.id%>").html("<%= j(render 'clients/errors', client: @client) %>");
  <% end %>
<% end %> 
